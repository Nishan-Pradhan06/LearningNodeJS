    .skills-details .boxes {
        width: 45%;
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
    }

    .skills-details .box {
        width: calc(100% / 2 - 20px);
        margin: 20px 0;
    }

    .skills-details .boxes .topic {
        font-size: 25px;
        color: #183153;
    }

    .skills-details .boxes .per {
